<Window x:Class="FHXTools.Views.ObjectLoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FHXTools.Views"
        mc:Ignorable="d"
        Title="ProgressWindow" Height="100" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        
        <DockPanel VerticalAlignment="Top" Grid.Row="1">
            <ProgressBar Name="pbPercent" HorizontalAlignment="Left" Height="20" VerticalAlignment="Center" Width="150"/>
            <Label Name="labelPercent"  Content="Label" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        </DockPanel>
        <Label Name="labelState" Content="Label" Grid.Row="0" VerticalAlignment="Bottom"/>
    </Grid>
    
</Window>
